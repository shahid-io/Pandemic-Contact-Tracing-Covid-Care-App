<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covidcare">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/c3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidCare"
        android:usesCleartextTraffic="true">
        <activity android:name=".Worker_view_events"></activity>
        <activity android:name=".Worker_duties" />
        <activity android:name=".Worker_manage_containment_zone" />
        <activity android:name=".Worker_home" />
        <activity android:name=".Worker_view_profile" />
        <activity android:name=".User_view_events" />
        <activity android:name=".User_manage_events" />
        <activity android:name=".User_book_vaccine" />
        <activity android:name=".User_view_vaccine_available_hospitals" />
        <activity android:name=".Login" />
        <activity android:name=".IPSetting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Users_home" />
        <activity android:name=".User_registration" />
        <activity android:name=".User_view_nearby_containment_zone" />
        <activity android:name=".User_view_notification" />
        <activity android:name=".MainActivity" />

        <service android:name=".LocationService" />
        <service android:name=".SocialDistanceAlert" />
    </application>

</manifest>